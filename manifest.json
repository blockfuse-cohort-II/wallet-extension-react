{
    "manifest_version": 3,
    "name": "Katera",
    "version": "1.0.0",
    "permissions": ["storage"],
    "host_permissions": ["<all_urls>"],
    "icons": {
      "16": "src/assets/icons/icon16.png",
      "48": "src/assets/icons/icon48.png",
      "128": "src/assets/icons/icon128.png"
    },
    "action": { 
      "default_icon": {
        "16": "src/assets/icons/icon16.png",
        "48": "src/assets/icons/icon48.png",
        "128": "src/assets/icons/icon128.png"
      },
      "default_popup": "index.html" 
    }
  }